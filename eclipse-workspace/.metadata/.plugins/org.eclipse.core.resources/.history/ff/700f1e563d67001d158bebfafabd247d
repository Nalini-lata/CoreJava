package DemoPack;

public class InterviewMinimumNumber {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		/*
		 * 2 4 5 
		 * 3 4 7
		 * 1 2 9
		 */
		
		int k= 0;
		int a[][]= {{2,4,5},{3,4,7},{1,2,9}};
		
		
		for(int i=0; i<3; i++) {
			
			for(int j=0; j<3; j++) {
				
				if(a[i][j]> k)
				
				System.out.println(a[i][j]);
				
				else
					System.out.println(k);
			}
		}

	}

}
