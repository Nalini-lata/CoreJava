package DemoPack;

public class Pb1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		//Nested Loops
		
		// Outer for loop
		//inner for loop
		
		for(int i=1; i<=4;i++) {
			
			System.out.println("outer loop");
			
			for(int j=1; j<=4;j++) {
				
				
				System.out.println("inner loop");
				
			}
		}

	}

}
